Sub Aim_Chat_Send_Normal(TheMessage$)
Dim aimchatwnd&
Dim wndateclass&
Dim ateclass&
Dim sendbuttonicon1&
Dim oscariconbtn&
aimchatwnd& = FindWindow("aim_chatwnd", vbNullString)
wndateclass& = FindWindowEx(aimchatwnd&, 0&, "wndate32class", vbNullString)
