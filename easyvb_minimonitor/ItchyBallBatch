Sub Form_Move(Frm As Form)
'Example:  In a label's mousedown event put this code
'Call FormMove(me)
Dim dragme&
ReleaseCapture
dragme& = SendMessage(Frm.hwnd, &H112, &HF012, 0)
End Sub
Sub Form_Fade_Blue(Frm As Object)
'This was taken from gumboaim.bas
    On Error Resume Next
    Dim intLoop As Integer
    Frm.DrawStyle = vbInsideSolid
    Frm.DrawMode = vbCopyPen
    Frm.ScaleMode = vbPixels
    Frm.DrawWidth = 2
    Frm.ScaleHeight = 256
    For intLoop = 0 To 255
    Frm.Line (0, intLoop)-(Screen.Width, intLoop - 1), RGB(0, 0, 255 - intLoop), B
    Next intLoop
End Sub
Sub Form_Fade_Green(Frm As Object)
'This was taken from gumboaim.bas
    On Error Resume Next
    Dim intLoop As Integer
    Frm.DrawStyle = vbInsideSolid
    Frm.DrawMode = vbCopyPen
    Frm.ScaleMode = vbPixels
    Frm.DrawWidth = 2
    Frm.ScaleHeight = 256
    For intLoop = 0 To 255
    Frm.Line (0, intLoop)-(Screen.Width, intLoop - 1), RGB(0, 255 - intLoop, 0), B
    Next intLoop
End Sub
Sub Form_Fade_Red(Frm As Object)
'This was taken from gumboaim.bas
    On Error Resume Next
    Dim intLoop As Integer
    Frm.DrawStyle = vbInsideSolid
    Frm.DrawMode = vbCopyPen
    Frm.ScaleMode = vbPixels
    Frm.DrawWidth = 2
    Frm.ScaleHeight = 256
    For intLoop = 0 To 255
    Frm.Line (0, intLoop)-(Screen.Width, intLoop - 1), RGB(255 - intLoop, 0, 0), B
    Next intLoop
End Sub
Sub Form_Fade_Fire(Frm As Object)
'This was taken from gumboaim.bas
    On Error Resume Next
    Dim intLoop As Integer
    Frm.DrawStyle = vbInsideSolid
    Frm.DrawMode = vbCopyPen
    Frm.ScaleMode = vbPixels
    Frm.DrawWidth = 2
    Frm.ScaleHeight = 256
    For intLoop = 0 To 255
    Frm.Line (0, intLoop)-(Screen.Width, intLoop - 1), RGB(255, 255 - intLoop, 0), B
    Next intLoop
End Sub
Sub Form_Fade_Silver(Frm As Object)
'This was taken from gumboaim.bas
    On Error Resume Next
    Dim intLoop As Integer
    Frm.DrawStyle = vbInsideSolid
    Frm.DrawMode = vbCopyPen
    Frm.ScaleMode = vbPixels
    Frm.DrawWidth = 2
    Frm.ScaleHeight = 256
    For intLoop = 0 To 255
    Frm.Line (0, intLoop)-(Screen.Width, intLoop - 1), RGB(255 - intLoop, 255 - intLoop, 255 - intLoop), B
    Next intLoop
End Sub
Sub Form_Fade_Ice(Frm As Object)
'This was taken from gumboaim.bas
    On Error Resume Next
    Dim intLoop As Integer
    Frm.DrawStyle = vbInsideSolid
    Frm.DrawMode = vbCopyPen
    Frm.ScaleMode = vbPixels
    Frm.DrawWidth = 2
    Frm.ScaleHeight = 256
    For intLoop = 0 To 255
    Frm.Line (0, intLoop)-(Screen.Width, intLoop - 1), RGB(0, 255 - intLoop, 255), B
    Next intLoop
End Sub
Function Get_Time()
Get_Time = Time
End Function
Function Get_Date()
Get_Date = Date
End Function
Sub Computer_PlayWav(TheWav$)
Dim wav&
wav& = sndPlaySound(TheWav$, 1):
End Sub
